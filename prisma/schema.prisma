
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
model Feedback {
  id       String   @id @default(cuid())
  userId   String
  username String
  roomname String
  text     String
  createdAt DateTime @default(now())
}

model Link {
  id       String   @id @default(cuid())
  userId   String
  username String
  roomname String
  text     String
  filename String
  createdAt DateTime @default(now())
}
